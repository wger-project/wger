{
  "name": "wger",
  "version": "2.1.dev1",
  "description": "Self hosted FLOSS fitness/workout and weight tracker",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wger-project/wger/wger.git"
  },
